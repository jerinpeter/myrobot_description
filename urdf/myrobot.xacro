<link name="lidar_1">
  <inertial>
    <origin rpy="0 0 0" xyz="-1.3877787807814457e-17 3.404849079029522e-16 0.012500000000000927"/>
    <mass value="0.10794885113050198"/>
    <inertia ixx="4.1e-05" ixy="-0.0" ixz="0.0" iyy="4.1e-05" iyz="-0.0" izz="7e-05"/>
